{
  "name": "ken-vde/composer-extend-class",
  "type": "composer-plugin",
  "description": "Composer plugin for extending any package class",
  "keywords": [
    "Composer",
    "extending",
    "class"
  ],
  "authors": [
    {
      "name": "Ken van der Eerden"
    }
  ],
  "require": {
    "composer-plugin-api": "^2.3.0"
  },
  "require-dev": {
    "composer/composer": "^2.3.0"
  },
  "autoload": {
    "psr-4": {
      "ComposerExtendClass\\": "src/"
    }
  },
  "extra": {
    "class": [
      "ComposerExtendClass\\Plugin"
    ]
  },
  "prefer-stable": true
}
